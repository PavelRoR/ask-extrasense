@import '../_dev/css/bootstrap.min.css'

*
	margin 0
	padding 0
	outline none
body
	font-family: 'OpenSans', sans-serif;
	font-size: 16px
	background-color rgba(159,196,225,.2)
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
button
	border none
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	margin 0 auto
	width 100%
	max-width 240px
	border-radius: 10px;
	background-color: #f2c22c
	height: 60px;
	line-height: 60px;
	font-size: 15px;
	font-family: "OpenSansBold", sans-serif;
	color: #11191b
	text-align: center;
	cursor pointer
	text-transform: uppercase
	transition: all .2s ease-in-out
	&:focus
		color #11191b
		background-color: #f2c22c
		text-decoration: none 
	&:hover
		color #ffffff
		background-color: #11191b
		text-decoration: none
		box-shadow: 0 0 10px 5px rgba(#fff, .5)
#main
#main_thanks
	width 100%
	padding 0 0 90px
	background-color: #052553
	background-image url(../img/main_bg.jpg)
	background-repeat no-repeat
	background-position: center
	background-size: cover
	position relative
	&:before
		content: ''
		display block
		position: absolute
		width 100%
		height 100%
		top 0
		left 0
		bottom 0
		right 0
		background-color: rgba(#000, .8)
		z-index 0
	.container
