	color #ffffff
	font-family: 'OpenSansCondensedBold', sans-serif
	font-size: 28px
	text-align: center
	width 100%
	max-width 58%
.mail_subtitle 
	color #ffb21e
	font-size: 22px
	text-align: center
.mail
	border-radius: 5px
	border: 1px solid #e8e8e8
	height 51px
	line-height: 51px
	padding-left:  20px
	color: rgba(24, 24, 24, 0.6);
	width 100%
	max-width 252px
	margin 20px auto
	font-family: 'OpenSans', sans-serif
	display block
	&:focus
	&:active
		background-color #fff
.button_mail 
	max-width: 252px;
	height: 51px;
	line-height: 51px;
.alert_message {
	display: none;
	text-align: center;
	font-size: 16px;
	padding-top: 10px;
	font-family: 'OpenSansLight', sans-serif;
	color: #ff0000
}
.checking {
	display: table;
	width 100%
	max-width: 80%
	margin: 25px auto 0;
	text-align: left
}

.checking .check_label {
	font-size: 12px;
	font-family: "OpenSansLight", sans-serif;
	color: #fff
	line-height: 14px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 25px;
	text-align: left
}

.checking .check_label a {
	text-decoration: underline
	color #fff
}
.checking .check_label a:focus
	opacity: 1
	color #fff
.checking .check_label a:hover
	opacity: .6
	color #fff
.checking .check_label:before {
	content: '';
	background-color: #aaa;
	position: absolute;
	left: 0;
	top: 0;
	width: 14px;
	height: 14px;

}

.checking .check_label:after {
	content: '';
	position: absolute;
	left: 5px;
	top: 2px;
	width: 5px;
	height: 10px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 3px solid #1f2022
	border-right: 3px solid #1f2022
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + label:before {
	border: none
	background-color: #f6f5f5

}

.check:checked + label:after {
	opacity: 1;
	top: 2px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}

@-webkit-keyframes autofill {
	to {
		color: rgba(29, 28, 24, 0.702);
		background: #fff;
	}
}

input:-webkit-autofill {
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;

}

